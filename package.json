{
  "name": "prisma-schema-transformer",
  "description": "Transform names from snake_case to camelCase in Prisma schema.",
  "version": "0.8.1",
  "repository": "https://github.com/IBM/prisma-schema-transformer",
  "author": {
    "name": "Michael Lin",
    "email": "hi@michaellin.me",
    "url": "https://michaellin.me"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "prisma-schema-transformer": "bin.js"
  },
  "files": [
    "dist",
    "bin.js"
  ],
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": ".build/"
      }
    },
    "require": [
      "ts-node/register"
    ]
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tscconfig.json",
    "test": "DATABASE_URL=postgresql://localhost:5432/prisma ava",
    "lint": "xo --fix",
    "prepare": "yarn build",
    "schema": "cp fixtures/simple.prisma fixtures/newSimple.prisma && yarn prisma-schema-transformer fixtures/newSimple.prisma",
    "migrate": "yarn schema && yarn prisma migrate dev --name",
    "dev": "ts-node-dev -r tsconfig-paths/register invoke.ts"
  },
  "dependencies": {
    "@prisma/engine-core": "4.11.0",
    "@prisma/generator-helper": "4.0.0",
    "@prisma/internals": "4.0.0",
    "arg": "^4.1.3",
    "camelcase": "^6.0.0",
    "dotenv": "^8.2.0",
    "immer": "^9.0.6",
    "jest": "^29.5.0",
    "pluralize": "^8.0.0",
    "prisma": "4.11.0",
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "@types/pluralize": "^0.0.29",
    "ava": "^3.8.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^4.5.3",
    "xo": "^0.32.0"
  }
}
